{
    "name": "tri/todo-list-app",
    "description": "Simple todo list app",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Tri\\Core\\": "core/"
        },
        "files": [
            "app/init.php",
            "app/validation.php",
            "app/store.php",
            "app/update.php",
            "app/destroy.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "tri-suli",
            "email": "trisulipras@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6"
    }
}
